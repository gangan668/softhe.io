# Environment Variables for Softhe.io React App
# Copy this file to .env and fill in your actual values

# Google Analytics
# Get your GA4 Measurement ID from: https://analytics.google.com/
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# API Configuration (if you have a backend)
# VITE_API_URL=https://api.softhe.io
# VITE_API_TIMEOUT=5000

# Stripe Configuration (Public Keys Only)
# Get your keys from: https://dashboard.stripe.com/apikeys
# VITE_STRIPE_PUBLISHABLE_KEY=pk_live_XXXXXXXXXXXXXXXXXXXXXXXX

# Application Settings
VITE_APP_NAME=Softhe.io
VITE_APP_URL=https://softhe.io
VITE_SUPPORT_EMAIL=support@softhe.io

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Development Settings
# VITE_DEV_MODE=true
# VITE_LOG_LEVEL=debug

# Social Media Links
VITE_TWITTER_URL=https://x.com/SoftheCS
VITE_DISCORD_URL=https://discord.com/users/softhecs
VITE_GITHUB_URL=https://github.com/Softhe
VITE_YOUTUBE_URL=https://www.youtube.com/@softhe

# Contact Information
VITE_CONTACT_EMAIL=support@softhe.io
VITE_DISCORD_USERNAME=@softhecs

# Performance Monitoring (Optional)
# VITE_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
# VITE_SENTRY_ENVIRONMENT=production

# Rate Limiting Configuration
VITE_RATE_LIMIT_MAX_ATTEMPTS=3
VITE_RATE_LIMIT_WINDOW_MS=60000

# IMPORTANT NOTES:
# - Never commit the actual .env file to version control
# - Only expose VITE_ prefixed variables to the client
# - Keep sensitive keys (API secrets, private keys) on the server
# - Use different values for development and production environments
